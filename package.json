{
  "name": "koa-test-demo",
  "version": "1.0.0",
  "description": "执行shell脚本示例, HTTP状态码示例（Nginx演示）, 文件下载示例（各种格式和分片下载）",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon app.js",
    "prod": "cross-env NODE_ENV=prod node app.js",
    "start": "npm-run-all -p dev startNginx",
    "startProd": "npm-run-all -p prod startNginx",
    "startNginx": "cd nginx && start nginx.exe",
    "reloadNginx": "cd nginx && nginx.exe -s reload",
    "stopNginx": "cd nginx && nginx.exe -s stop",
    "eslintInit": "eslint --init"
  },
  "keywords": [],
  "author": "lyn",
  "license": "MIT",
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-helmet": "^7.0.2",
    "koa-jwt": "^4.0.4",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "log4js": "^6.9.1",
    "mime": "^3.0.0",
    "socket.io": "^4.7.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}
